{
  "name": "universal",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "dependencies": {
    "express": "^4.13.4",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "swig": "^1.4.2"
  },
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "cpx": "^1.3.1",
    "proxy-middleware": "^0.15.0",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "webpack": "^1.12.15",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build-server": "mkdir -p dist/server/views && cp src/server/views/** dist/server/views && ./node_modules/.bin/babel src -d dist",
    "build-client": "webpack --config config/webpack.config.js",
    "watch-server": "./node_modules/.bin/cpx --watch \"src/server/views/**\" dist/server/views & ./node_modules/.bin/babel --watch src -d dist",
    "watch-client": "node dist/server/dev.js",
    "start": "npm run clean && npm run build-server && (npm run watch-server & nodemon -e js,swig --watch dist/server dist/server/serve.js & npm run watch-client)"
  },
